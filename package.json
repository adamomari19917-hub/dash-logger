{
  "name": "logger-bot-dashboard",
  "version": "1.0.0",
  "description": "Logger Bot Dashboard - Full Stack Application",
  "main": "server/main.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "npm run server:start",
    "build": "cd client && npm run build && mkdir -p ../public && cp -r build/* ../public/",
    "install:all": "npm install && npm run client:install && npm run server:install",
    "client:install": "cd client && npm install",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:test": "cd client && npm test",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && nodemon main.js",
    "server:start": "cd server && node main.js",
    "server:test": "cd server && npm test",
    "heroku-postbuild": "npm run client:install && npm run client:build"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "discord",
    "bot",
    "logging",
    "dashboard",
    "react",
    "nodejs",
    "express"
  ],
  "author": "Logger Bot Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/logger-bot-dashboard.git"
  }
}